{
  "name": "react-wordpress",
  "version": "0.1.0",
  "license": "MIT",
  "main": "server/index.js",
  "private": true,
  "dependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/node": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-object-assign": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/preset-env": "^7.2.0",
    "@babel/preset-react": "^7.0.0",
    "@emotion/core": "^10.0.5",
    "@emotion/styled": "^10.0.5",
    "@loadable/babel-plugin": "^5.10.0",
    "@loadable/component": "^5.10.1",
    "@loadable/server": "^5.9.0",
    "axios": "^0.18.0",
    "babel-plugin-emotion": "^10.0.5",
    "compression": "^1.7.3",
    "connected-react-router": "^6.0.0",
    "cpx": "^1.5.0",
    "emotion": "^10.0.5",
    "emotion-server": "^10.0.5",
    "emotion-theming": "^10.0.5",
    "eslint-config-standard": "^11.0.0",
    "eslint-config-standard-react": "^1.1.6",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-standard": "^3.0.1",
    "express": "^4.16.4",
    "firebase-admin": "~7.0.0",
    "firebase-functions": "^2.2.0",
    "gsap": "^2.1.3",
    "history": "^4.7.2",
    "husky": "^1.2.1",
    "nodemon": "^1.18.9",
    "path": "^0.12.7",
    "polished": "^2.3.1",
    "prop-types": "^15.6.2",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-helmet": "^5.2.0",
    "react-loadable": "^5.5.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-scripts": "2.1.1",
    "react-scroll": "^1.7.11",
    "react-slick": "^0.24.0",
    "scroll-into-view": "^1.9.3",
    "template-file": "^3.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "copy:build": "cpx 'build/**/**/**/*' functions/build",
    "clean": "rimraf functions/build/*",
    "build:client": "react-scripts build",
    "build:server": "babel server -d functions/server --plugins=@loadable/babel-plugin,@babel/plugin-syntax-dynamic-import,@babel/proposal-class-properties,@babel/plugin-transform-object-assign,@babel/plugin-transform-runtime,emotion --copy-files --ignore node_modules",
    "package-server": "babel server.js --out-file functions/index.js --plugins=@loadable/babel-plugin,@babel/plugin-syntax-dynamic-import,@babel/proposal-class-properties,@babel/plugin-transform-object-assign,@babel/plugin-transform-runtime,emotion",
    "package-src": "babel src -d functions/src --plugins=@loadable/babel-plugin,@babel/plugin-syntax-dynamic-import,@babel/proposal-class-properties,@babel/plugin-transform-object-assign,@babel/plugin-transform-runtime,emotion --copy-files --ignore node_modules",
    "test": "react-scripts test",
    "pressr": "yarn build:client",
    "ssr": "nodemon --exec babel-node --plugins=@loadable/babel-plugin,@babel/plugin-syntax-dynamic-import,@babel/proposal-class-properties,@babel/plugin-transform-object-assign,@babel/plugin-transform-runtime,emotion ./server/index.js",
    "preserve": "yarn clean && yarn build:client && yarn build:server && yarn package-src && yarn copy:build",
    "serve": "firebase serve --only functions,hosting",
    "deploy": "firebase deploy",
    "predeploy": "yarn preserve"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 10",
    "not op_mini all"
  ]
}
